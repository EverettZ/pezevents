{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "pezevents-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/pezevents-app",
      "sourceRoot": "projects/pezevents-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/pezevents-app",
            "index": "projects/pezevents-app/src/index.html",
            "main": "projects/pezevents-app/src/main.ts",
            "polyfills": "projects/pezevents-app/src/polyfills.ts",
            "tsConfig": "projects/pezevents-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/pezevents-app/src/favicon.ico",
              "projects/pezevents-app/src/assets"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "projects/pezevents-app/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/pezevents-app/src/environments/environment.ts",
                  "with": "projects/pezevents-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "pezevents-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "pezevents-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "pezevents-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/pezevents-app/src/test.ts",
            "polyfills": "projects/pezevents-app/src/polyfills.ts",
            "tsConfig": "projects/pezevents-app/tsconfig.spec.json",
            "karmaConfig": "projects/pezevents-app/karma.conf.js",
            "assets": [
              "projects/pezevents-app/src/favicon.ico",
              "projects/pezevents-app/src/assets"
            ],
            "styles": [
              "projects/pezevents-app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/pezevents-app/tsconfig.app.json",
              "projects/pezevents-app/tsconfig.spec.json",
              "projects/pezevents-app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/pezevents-app/e2e/protractor.conf.js",
            "devServerTarget": "pezevents-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "pezevents-app:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {}
        }
      }
    },
    "pezevents-lib": {
      "projectType": "library",
      "root": "projects/pezevents-lib",
      "sourceRoot": "projects/pezevents-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/pezevents-lib/tsconfig.lib.json",
            "project": "projects/pezevents-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/pezevents-lib/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/pezevents-lib/src/test.ts",
            "tsConfig": "projects/pezevents-lib/tsconfig.spec.json",
            "karmaConfig": "projects/pezevents-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/pezevents-lib/tsconfig.lib.json",
              "projects/pezevents-lib/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "pezevents-app",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}